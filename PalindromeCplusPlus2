#include <iostream>
#include <string>

int main()
{
    //int x = 342;

    std::string xa = "";

    std::cin >> xa;

    std::string compare = "";

    //xa = std::to_string(x);



    for (int i = 0; i < xa.length(); i++)
    {
        compare = compare + xa.at(xa.length() - (i + 1));

    }

    if (xa == compare)
    {
        std::cout << "true" << std::endl;
    }
    else
    {
        std::cout << "false" << std::endl;
    }
}
